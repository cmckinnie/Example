@model OdeToFood.ViewModels.HomePageViewModel
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>OdeToFood</title>
</head>
<body>
    <h1>@Model.CurrentMessage</h1>
    <table>
        @foreach (var restuarant in Model.Restaurants)
        {
            <tr>
                <td>@restuarant.Id</td>
                <td>@restuarant.Name</td>
            </tr>
        }
    </table>
</body>
</html>